@model IEnumerable<UCP1_PAW_121_C.Models.Jadwal>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Jam")
            </th>
            <th>
                @Html.DisplayName("Tanggal Sewa")
            </th>
            <th>
                @Html.DisplayName("Tujuan")
            </th>
            <th>
                @Html.DisplayName("Id Kota Tujuan")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Jam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TglSewa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tujuan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdKotaTujuanNavigation.IdKotaTujuan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdJadwal"><i class="fa fa-marker fa-lg"></i></a> |
                <a asp-action="Details" asp-route-id="@item.IdJadwal"><i class="fas fa-list"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.IdJadwal"><i class="fa fa-trash-alt fa-lg"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
